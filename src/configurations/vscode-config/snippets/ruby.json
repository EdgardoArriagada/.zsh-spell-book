{
  "Print to console": {
    "prefix": "puts",
    "body": [
      "puts \"$1\"",
      "$2"
    ],
    "description": "Log output to console"
  },
  "Smart Print": {
    "prefix": "le",
    "body": [
      "puts \"le $1: #{$1$2}\"",
      "$3"
    ],
    "description": "Smart print"
  },
  "Hacky Log": {
    "prefix": "log",
    "body": [
      "$$console.log(\"$1\")",
      "$3"
    ],
    "description": "Hacky log print"
  },
  "Hacky smart Log": {
    "prefix": "leg",
    "body": [
      "$$console.log(\"le $1: #{$1$2}\")",
      "$3"
    ],
    "description": "Hacky log print"
  },
  "Context Block": {
    "prefix": "context",
    "body": [
      "context '$1' do",
      "  $2",
      "end"
    ],
    "description": "generate context block"
  },
  "It Block": {
    "prefix": "it",
    "body": [
      "it '$1' do",
      "  $2",
      "end"
    ],
    "description": "generate it block"
  }
}

