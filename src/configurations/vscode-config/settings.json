    "window.zoomLevel": 0,
    "vim.vimrc.enable": true,
    "editor.lineNumbers": "relative",
    "cSpell.userWords": [
        "Before",
        "Bounding",
        "Closest",
        "Drag",
        "Elements",
        "LEGALTECH",
        "Props",
        "depends",
        "draggable",
        "draggables",
        "dragging",
        "height",
        "self",
        "sidekiq",
        "spody",
        "tmpfs",
        "unmount",
        "wfile"
